parameters:
    level: 1
    paths:
        - app/
    bootstrapFiles:
        - bootstrap/app.php
    excludePaths:
        - tests/
        - database/
        - storage/
        - vendor/
        - app/Http/Controllers/Api/BaseApiController.php
        - app/Services/Auth/AuthService.php
    ignoreErrors:
        - '#Call to an undefined (method|static method) (Illuminate|App)\\.*::#'
        - '#Cannot call method .* on (Illuminate|App)\\.*#'
        - '#Access to an undefined property (Illuminate|App)\\.*#'
        - '#Parameter .* expects .*, .* given#'
        - '#Method .* has no return type specified#'
        - '#has parameter .* with no value type specified in iterable type array#'
        - '#Call to an undefined method .* on an unknown class .*#'
        - '#Call to static method .* on an unknown class .*#'
        - '#Class .* not found#'
        - '#Call to an undefined static method Spatie\\Permission\\Models\\Role::where\\(\\)#'
        - '#Call to an undefined method Laravel\\Sanctum\\Contracts\\HasAbilities::delete\\(\\)#'
        - '#Parameter .* is not contravariant with parameter.*#'
        - '#overrides method .* but misses parameter.*#'
        - '#Call to an undefined method .*::findOrFail\(\)#'
        - '#Call to an undefined method .*::where\(\)#'
        - '#Call to an undefined method .*::create\(\)#'
        - '#Call to an undefined method .*::find\(\)#'
